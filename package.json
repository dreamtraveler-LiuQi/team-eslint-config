{
  "name": "team-eslint-config",
  "version": "1.0.31",
  "description": "Team ESLint and Prettier configuration for Vue, Nuxt, and WXT projects",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "team-eslint-setup": "./dist/bin/install.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "clean": "rimraf dist",
    "lint": "eslint \"src/**/*.{ts,vue}\"",
    "lint:fix": "eslint \"src/**/*.{ts,vue}\" --fix",
    "format": "prettier --write \"src/**/*.{ts,vue,json,md}\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "prepare": "husky install && npm run build",
    "prepublishOnly": "npm run clean && npm run build",
    "release": "semantic-release",
    "type-check": "vue-tsc --noEmit"
  },
  "keywords": [
    "eslint",
    "prettier",
    "config",
    "vue",
    "nuxt",
    "wxt",
    "typescript"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/dreamtraveler-LiuQi/team-eslint-config"
  },
  "homepage": "https://github.com/dreamtraveler-LiuQi/team-eslint-config",
  "bugs": {
    "url": "https://github.com/dreamtraveler-LiuQi/team-eslint-config/issues"
  },
  "author": "Your Team",
  "license": "MIT",
  "peerDependencies": {
    "@commitlint/cli": ">=19.0.0",
    "eslint": "^8.56.0",
    "husky": ">=8.0.0",
    "prettier": ">=3.0.0",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0"
  },
  "dependencies": {
    "@commitlint/config-conventional": "^19.0.0",
    "@commitlint/types": "^19.0.0",
    "@nuxtjs/eslint-config-typescript": "^12.1.0",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.2",
    "eslint-plugin-vue": "^9.20.0",
    "vue-eslint-parser": "^9.4.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.0.0",
    "@types/eslint": "^8.56.0",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.11.0",
    "@types/prettier": "^3.0.0",
    "eslint": "^8.56.0",
    "husky": "^8.0.0",
    "jest": "^29.7.0",
    "prettier": "^3.2.0",
    "rimraf": "^5.0.5",
    "semantic-release": "^23.0.0",
    "ts-jest": "^29.1.1",
    "typescript": "~5.3.3",
    "vue-tsc": "^1.8.27"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "lint-staged": {
    "*.{js,ts,vue}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "testMatch": [
      "**/test/**/*.test.ts"
    ],
    "collectCoverageFrom": [
      "src/**/*.ts",
      "!src/types.ts",
      "!src/**/*.d.ts"
    ]
  },
  "release": {
    "branches": [
      "main"
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/npm",
      "@semantic-release/github"
    ]
  }
}
