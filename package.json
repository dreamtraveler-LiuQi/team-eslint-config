{
  "name": "team-eslint-config",
  "version": "1.0.0",
  "description": "Team ESLint and Prettier configuration for Vue, Nuxt, and WXT projects",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "team-eslint-setup": "./dist/bin/install.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "concurrently \"tsc --watch\" \"nodemon --watch src --ext .ts --exec npx eslint src/\"",
    "lint": "eslint --ext .ts src/",
    "lint:fix": "eslint --ext .ts src/ --fix",
    "type-check": "tsc --noEmit",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "eslint",
    "prettier",
    "config",
    "vue",
    "nuxt",
    "wxt",
    "typescript"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/xxx/shang-utils"
  },
  "homepage": "https://github.com/xxx/shang-utils",
  "bugs": {
    "url": "https://github.com/xxx/shang-utils/issues"
  },
  "author": "Your Team",
  "license": "MIT",
  "peerDependencies": {
    "eslint": ">=8.0.0",
    "prettier": ">=2.0.0"
  },
  "dependencies": {
    "@nuxtjs/eslint-config-typescript": "^12.0.0",
    "@typescript-eslint/eslint-plugin": "^5.59.0",
    "@typescript-eslint/parser": "^5.59.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-mozilla": "^3.2.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.11.0"
  },
  "devDependencies": {
    "@types/node": "^18.15.11",
    "@types/prettier": "^2.7.3",
    "@types/eslint": "^8.37.0",
    "concurrently": "^8.0.1",
    "eslint": "^8.38.0",
    "nodemon": "^2.0.22",
    "prettier": "^2.8.7",
    "typescript": "^5.0.4"
  }
}
